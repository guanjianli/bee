page{
  background: #f4efe7;
}

.feed {
    margin: 20rpx;
    display: block
}

.feedAd {
    text-align: center
}


.small_img{
  width: 90rpx;
  height: 120rpx;
  max-height: 120rpx;
}

.big_img{
  width: 100%;
  height: 282rpx;
  display: block;
}

.feedBook .main {
    display: block;
    position: relative;
    padding: 16rpx 20rpx 12rpx 130rpx;
    min-height: 134rpx;
    background: #fafaf6;
    /* border: 2rpx solid #dfdbce; */
    border-radius: 10rpx;
    -webkit-box-shadow: 0 2rpx 6rpx rgba(0,0,0,.1);
    -moz-box-shadow: 0 2rpx 6rpx rgba(0,0,0,.1);
    box-shadow: 0 2rpx 6rpx rgba(0,0,0,.1)
}

.feedBook .cover {
    position: absolute;
    top: 20rpx;
    left: 24rpx
}

.feedBook .cover .img {
    width: 90rpx;
    height: 120rpx;
    background: url(data:image/png;base64,) no-repeat center;
    background-size: 100%
}

.feedBook .cover img {
    width: 100%;
    height: 100%
}

.feedBook .h4 {
    padding-bottom: 88rpx;
    font-size: 28rpx;
    color: #555;
    line-height: 44rpx
}

.feedBook .desc,.feedBook .info,.feedBook .temp-free {
    overflow: hidden;
    height: 44rpx;
    line-height: 44rpx;
    font-size: 26rpx;
    color: #a4795f;
    text-overflow: ellipsis;
    white-space: nowrap
}

.feedBook .info {
    position: absolute;
    bottom: 58rpx;
    left: 130rpx;
    right: 20rpx
}

.feedBook .desc,.feedBook .temp-free {
    position: absolute;
    bottom: 14rpx;
    left: 130rpx;
    right: 20rpx
}

.feedBook .temp-free {
    color: #43c38e
}

.feedBook .temp-free .price {
    padding-right: 20rpx;
    color: #b1b1b1;
    text-decoration: line-through
}

.feedBook.feedBook_simple .main {
    padding-left: 20rpx;
    min-height: 94rpx
}

.feedBook.feedBook_simple h4 {
    padding-bottom: 44rpx
}

.feedBook.feedBook_simple .desc {
    left: 20rpx
}

.feedBook .ranking {
    position: absolute;
    top: -2rpx;
    left: -2rpx;
    z-index: 9
}

.feedBook .ranking .i {
    vertical-align: top
}

.feedBook .free {
    position: absolute;
    top: -10rpx;
    right: -12rpx
}

.feedBook .sign {
    position: absolute;
    top: -6rpx;
    right: -4rpx
}

.feedBook_topic .main {
    min-height: inherit;
    padding: 20rpx 60rpx 20rpx 20rpx
}

.feedBook_topic .main h4 {
    padding-bottom: 44rpx
}

.feedBook_topic .main .desc {
    left: 20rpx;
    right: 60rpx
}

.feedBook_topic .toDetail {
    position: absolute;
    right: 20rpx;
    top: 50%;
    margin-top: -16rpx
}

.feedBook_topic .toDetail .i {
    vertical-align: top
}


/* 大图 */
.feedBook_list {
    display: block;
    position: relative;
    background: #fafaf6;
    border: 2rpx solid #dbd5c2;
    border-radius: 10rpx;
    overflow: hidden;
    /* box-sizing: border-box; */
}

.feedBook_list .main {
    padding: 0;
    border: none;
    -webkit-box-shadow: none;
    -moz-box-shadow: none;
    box-shadow: none;
    box-sizing: content-box;
}

.feedBook_list .image {
    width: 100%;
    max-height: 600rpx;
    overflow: hidden;
}

.shadow{
   position: relative;
}
.shadow:after {
    position: absolute;
    content: '';
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    box-shadow:0px 0.5px 4px 0px #be9f8c inset;
}

.feedBook_list .image img {
    width: 100%
}

.feedBook_list text {
    padding: 10rpx 36rpx;
    line-height: 1.5;
    text-align: left;
    color: #be9f8c;
    font-size: 26rpx;
    display:block;
}

.title_bar{
  width:305rpx;
  height: 85rpx; 
}

.footer{
  width: 750rpx;
  height: 110rpx;
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  color: #a4795f;
  box-shadow: 0 -1px 3px rgba(0,0,0,.1);
  border-top: 1px solid #dfdbce;
  background: #fafaf6;
}
.foot_item{
  flex: 1;
}
.foot_tx{
  font-size: 28rpx;
  margin-top:-20rpx;
}
.duck_img{
  width: 120rpx;
  height: 120rpx;
  margin-left: -15rpx; 
}